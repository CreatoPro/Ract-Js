
@media only screen and (min-device-width :1024px) and (max-device-width :1824px) {

  .LearningSteps .container {
      max-width: 1140px !important;
  }

  .mobile_course .head-title {
      color: #1E4471;
      font-size: 17px;
      width: 100% !important;
      padding: 0px;
      margin-left:10px;
  }

  .LearningSteps .grid .border-right{
      border-right:none !important;
  }

  .Learning{
      padding-bottom: 20px;
  }
  .Learning .card_list{
      width: 100%;
      margin: 0 auto !important;
      padding-left: 5%;
      padding-right: 5%;
  }

  .Learning .card{
      position: relative;
      clear: none;
      background-color: #fff;
      border: 1px solid #e5e5e5;
      width: 230px;
      float: left;
      margin: 6px;
  }

  /*.Learning .card {
      clear: none;
      width: 190px;
      float: left;
      height: 190px;
      background: #fff;
      border: 1px solid #f6f6f6;
      margin: 6px;
      border-radius: 7px;
      box-shadow: 0 5px 10px #555555;
  }
  */
  .Learning .cardbody {
      width: 100%;
      background: #fff;
      min-height: 60px;
      margin: 0px;
      padding: 0px;
  }

  .Learning .cardbody img {
      float: none;
      padding: 0px;
      border-radius: 1px;
      width: 100%;
      height: 160px;
  }

  .Learning .cardbody svg{
      display: none;
  }

  /*.Learning .cardbody h4.head {
      color: #333;
      float: left;
      margin: 12px;
      width: 94%;
      font-size: 15px;
  }*/


  .Learning .cardbody h4.head {
      color: #303030;
      font-size: 15px;
      line-height: 18px;
      font-weight: 400;
      height: 42px;
      overflow: hidden;
      padding-top: 5px;
      word-wrap: break-word;
      transition: .15s ease-in-out;
      display: block;
      display: -webkit-box;
      -webkit-line-clamp: 2;
      -webkit-box-orient: vertical;
      width:100% !important;
  }

  .Learning .card_list{
      margin: 30px 0px !important;
  }
  .Learning{
      background:#fff !important;	
  }

  .align-center{
      display:block;
  }

  .LearningAttachment .card .cardbody {
      width: 92%;
      background: #fff;
      min-height: 40px !important;
      margin: 0px auto;
      padding: 5px;
  }

}


@media only screen and (min-device-width :320px) and (max-device-width :600px) {
  .Learning .module-header-inner{
      height: 60px;
      text-align: center;
  }

  .Learning .module-header-inner img {
      width: 45px;
      height: auto;
      margin-top: 8px;
  }

  .Learning .arrow-set {
      top: 108px;
      background: #fff;
      padding: 6px 15px;
      width: 100%;
      margin: 0px;
  }
  .Learning .arrow-set {
      line-height: 30px;
  }

  .Learning .arrow-set h3 {
      font-size: 16px;
      color: #1E4471;
      margin: 0px;
      margin-top: 10px;
      text-align:left;
  }
  .light_blue {
      color: #78849E;
  }

  .Learning .arrow-set .border {
      border: 1px solid #DDDDDD;
      padding: 5px;
      height: 26px;
      margin: 5px;
      border-radius: 5px;
  }

  .resposive-image {
      width: 100%;
  }

  .mobile_course_list .resposive-image{
      width: 100%;
      height: 225px;
      object-fit: cover;
  }
  .LearningSteps .card .cardbody svg {
      float: right;
      margin: 5px;
      background: #A3A3A8;
      padding: 5px;
      width: 21px;
      height: 21px;
      border-radius: 50%;
      color: #fff;
  }
  .LearningSteps .card_list h4 {
      font-size: 16px;
      margin-top: 15px;
      margin-bottom: 5px;
      margin-left:0px;
  }

  .Learning .course_box {
      border-radius: 10px !important;
      overflow: hidden;
      box-shadow: 0px 3px 20px 0px rgba(202,214,224,0.5);
      margin-bottom:10px;
  }

  .Learning .course_box .card {
      border: 0px;
      padding: 0px !important;
  }

  .Learning .course_box .head {
      padding: 5px 10px;
      margin-top: -60px;
      background: #fff;
  }

  .mobile_course_list{
      padding:15px;
      /*background:#fff;*/
  }

  .mobile_course_list .course_box .head .title {
      color: #343A40;
  }
  .mobile_course_list p {
      font-size: 13px;
      margin-bottom: 5px;
  }

  .Learning .card_list .cardbody img{
    border-radius: 10px;
  }

}

.close {
  color: #fff; 
  opacity: 1;
}