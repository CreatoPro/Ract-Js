.home {
    width: 100%;
}

.home-container {
    height: 100vh;
}

.MuiGrid-container {
    width: 100%;
    display: flex;
    flex-wrap: wrap;
    box-sizing: border-box;
}

.home-col-left {
    background-size: cover;
    background-color: #fafafa;
    background-image: url(https://staticfiles.edusquares.com/images/login-background.png);
    /* background-image: url(https://source.unsplash.com/random); */
    background-repeat: no-repeat;
    background-position: center;
}

.MuiGrid-grid-xs-12 {
    flex-grow: 0;
    max-width: 100%;
    flex-basis: 100%;
}

.MuiGrid-container .login-form .form-control{
  height: 42px;
}

.MuiGrid-container .login-form .col-sm-4, .MuiGrid-container .login-form .col-sm-12{
  padding: 0px;
}


@media (min-width: 600px) {
    .MuiGrid-grid-sm-4 {
        flex-grow: 0;
        max-width: 33.333333%;
        flex-basis: 33.333333%;
    }

    .MuiGrid-grid-sm-8 {
        flex-grow: 0;
        max-width: 66.666667%;
        flex-basis: 66.666667%;
    }
}
@media (min-width: 960px) {
    .MuiGrid-grid-md-7 {
        flex-grow: 0;
        max-width: 58.333333%;
        flex-basis: 58.333333%;
    }

    .MuiGrid-grid-md-5 {
        flex-grow: 0;
        max-width: 41.666667%;
        flex-basis: 41.666667%;
    }
}
.MuiGrid-item {
    margin: 0;
    box-sizing: border-box;
}
