.home-col-right {
    color: rgba(0, 0, 0, 0.87);
    transition: box-shadow 300ms cubic-bezier(0.4, 0, 0.2, 1) 0ms;
    background-color: #fff;
}

.login-container {
    margin: 32px 32px; /*64px 32px;*/
    display: flex;
    align-items: center;
    flex-direction: column;
}

.login-icon-colorDefault {
    color: #fff;
    background-color: #bdbdbd;
}

.login-icon {
    margin: 8px;
    width: 40px;
    height: 40px;
    display: flex;
    overflow: hidden;
    position: relative;
    font-size: 1.25rem;
    align-items: center;
    flex-shrink: 0;
    font-family: "Roboto", "Helvetica", "Arial", sans-serif;
    line-height: 1;
    user-select: none;
    border-radius: 50%;
    justify-content: center;
}

.login-title {
    font-size: 1.5rem;
    font-family: "Roboto", "Helvetica", "Arial", sans-serif;
    font-weight: 400;
    line-height: 1.334;
    letter-spacing: 0em;
    margin: 0;
}

h1 {
    display: block;
    font-size: 2em;
    margin-block-start: 0.67em;
    margin-block-end: 0.67em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    font-weight: bold;
}


.login-form {
    width: 100%;
    margin-top: 8px;
}

.MuiFormControl-fullWidth {
    width: 100%;
}

.login-form.md-outline {
    width:100%;
}

.login-form>
.md-form.md-outline.form-lg .form-control.form-control-lg {
    padding: .725rem .725rem;
}


.MuiGrid-grid-xs-true {
    flex-grow: 1;
    max-width: 100%;
    flex-basis: 0;
}

.login-link {
    font-size: 0.875rem;
    font-family: "Roboto", "Helvetica", "Arial", sans-serif;
    font-weight: 400;
    line-height: 1.43;
    letter-spacing: 0.01071em;
    margin: 0;
    text-decoration: none;
    color: #1976d2;
}

.colorTextSecondary {
    color: rgba(0, 0, 0, 0.54);
}

.text-alignCenter {
    text-align: center;
}

.powered-by-container {
    background: #eee;
    bottom: 0px;
    /* position: fixed; */
    max-width: 100%;
    display: flex;
    justify-content: center;
    width: 100%;
}

.powered-by {
    padding: 8px;
    text-align: center;
}

.powered-by-text {    
    margin-bottom: 0px;
}

.off-line {
    background-color: #f44336;
    color: white;
    width: 100%;
    font-size: 12px;
    text-align: center;
}

.home .ilearn-blue-btn {
    background-image: -webkit-linear-gradient(0deg, #1d7ed5 0%, #15bae3 100%);
    border-color: #1d7ed5;
    /*z-index: 999999;*/
    display: block;
    position: relative;
}

.home .button-md {
    border-radius: 2px;
    overflow: hidden;
    height: 50px;
    font-size: 1.4rem;
    font-weight: 500;
    text-transform: uppercase;
    color: #fff;
    background-color: #488aff;
    box-shadow: 0 2px 2px 0 rgba(0, 0, 0, 0.14), 0 3px 1px -2px rgba(0, 0, 0, 0.2), 0 1px 5px 0 rgba(0, 0, 0, 0.12);
    transition: box-shadow 300ms cubic-bezier(0.4, 0, 0.2, 1), background-color 300ms cubic-bezier(0.4, 0, 0.2, 1), color 300ms cubic-bezier(0.4, 0, 0.2, 1);
    margin: 0.4rem 0rem;
    /* margin-right: 0.2rem;
    margin-left: 0.2rem; */
}

.home .button-full {
    display: block;
    width: 100%;
    contain: strict;
}

.home .button-inner {
    font-size: 1rem;
}

.margin-top-2 {
    margin-top: 2px !important;
}

.home .gradient-text {
    font-size: 1.6em;
    background: -webkit-linear-gradient(#1d7ed5, #15bae3);
    background-clip: border-box;
    background-clip: text;
    -webkit-background-clip: text;
    -webkit-text-fill-color: 
    transparent;
    text-align: center;
}

.institute_logo {
    max-width: 192px;
    max-height: 192px;
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3);    
}

.terms-and-conditions{
    text-align: center;
    color: red;
    padding: 5px;
}