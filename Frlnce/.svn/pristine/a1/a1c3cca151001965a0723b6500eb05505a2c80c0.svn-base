body.test-player-body
{
    color: #000000 !important;
    overflow:hidden ! important;
    font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
}

.test-player table > tbody > tr > td {
    font-family: Times New Roman;
    font-size: 12pt;
}

.test-player input, .test-player button, .test-player select, .test-player textarea {
    font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
    font-weight: normal;
}

.test-player p{font-family: Times New Roman;font-size: 12pt;}
.test-player input[type="radio"],.test-player input[type="checkbox"]{ margin: 0 ;line-height: normal ;}
.test-player .loading{
    vertical-align: middle;
    text-align: center;
    height:50px;
    width:300px;
    position:absolute;
    left:50%;
    top:40%;
    margin:-75px 0 0 -135px;
    padding-top: 25px;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    font-size: 14px;
}
.test-player .loading:before{
    content: url('http://tuningfork-static-file-server.s3.amazonaws.com/images/chat/avatar_load.gif') transparent no-repeat;
}
table.bordered-table{
    margin-top: 10px;
