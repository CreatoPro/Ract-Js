/*marquee*/
.test-player .marquee {
    height: 25px;
    overflow: hidden;
    position: relative;
    background-color: #fe3831;
    color: #fff;
    font-size: 14px;
    border-top-right-radius: 5px;
    -moz-border-top-right-radius: 5px;
