.row-header {
    left: 0;
    top: 0;
    position: absolute;
    z-index: 10;
    display: block;
    width: 100%;
}

.header-md::after {
    left: 0;
    bottom: -5px;
    background-position: left 0 top -2px;
    position: absolute;
    width: 100%;
    height: 5px;
    background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAHBAMAAADzDtBxAAAAD1BMVâ€¦h/TXEAAAAaSURBVAjXYxCEAgY4UIICBmMogMsgFLtAAQCNSwXZKOdPxgAAAABJRU5ErkJggg==);
    background-repeat: repeat-x;
    content: "";
}

.back-button {
    background:transparent;
    color: #fff;
    font-size: large;
}

.back-button.focus, .back-button:focus, .back-button:hover {
    color: #fff;
    text-decoration: none;
}

.institute-top {
    display: block;
    width: 100%;
    height: 200px;
    text-align: center;
    z-index: -1;
    /*background: url(https://webfront.edusquares.com/triangles/files/pwa/cover.jpg) no-repeat transparent;*/
    background: no-repeat transparent;
    background-size: cover;
    background-position: center;
}


.institute-top::before {
    position: absolute;
    content: '';
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}

.institute-top-logo {
    margin: auto;
    /*background: url(https://webfront.edusquares.com/triangles/files/pwa/logo.jpg) no-repeat transparent;*/
    background: no-repeat transparent;
    background-size: 100%;
    background-color: #FFF;
    width: 120px;
    height: 120px;
    z-index: 10;
    margin-top: -65px;
    -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3);
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3);
}

.institute-top-intro {
    margin-top: 10px;
}

.institute-top-intro h4 {
    font-size: 20px;
    text-align: center;
    font-weight: 500;
    margin: 24px 32px 16px 32px;
    color: #000000;
}

.content-md hr {
    background-color: rgba(0, 0, 0, 0.08);
}

hr {
    height: 1px;
    border-width: 0;
    -webkit-box-sizing: content-box;
    box-sizing: content-box;
}

.tutorial-bottom {
    width: 100%;
    display: block;
    margin: 16px 0 16px 0;
    background: #FFF;
    padding: 0 32px;
}

